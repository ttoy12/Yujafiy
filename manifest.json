{
    "manifest_version": 3,
    "name": "Yujafiy",
    "version": "1.0",
    "description": "Play sound from Chrome extension",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_icon": "voice.png",
      "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
            // "matches": ["https://canvas.ucsc.edu/courses/*/external_tools/6825"],
        }
    ]
}